{
  "name": "check-my-spots",
  "version": "1.0.0",
  "description": "Check, book and cancel spots on an event or place",
  "main": "index.html",
  "scripts": {
    "prestart": "npm install",
    "start": "http-server",
    "start-dev": "typings install && tsc -v && tsc && npm run build-css && concurrently \"npm run tsc:w\" \"npm run build-css:w\" && http-server -s",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "build-css": "node-sass scss --include-path Scss --recursive --output css",
    "build-css:w": "nodemon -e scss -x \"npm run build-css\"",
    "build-assets": "tsc && npm run build-css"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/javiBertos/checkMySpots.git"
  },
  "keywords": [
    "js",
    "backbone",
    "typescript",
    "sass"
  ],
  "author": "Javier Bertos <yo@javierbertos.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/javiBertos/checkMySpots/issues"
  },
  "homepage": "https://github.com/javiBertos/checkMySpots#readme",
  "dependencies": {
    "backbone": "^1.3.3",
    "http-server": "^0.9.0",
    "jquery": "^3.1.1",
    "systemjs": "^0.19.42",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "node-sass": "^4.3.0",
    "nodemon": "^1.11.0",
    "typescript": "^2.1.5",
    "typings": "^2.1.0"
  }
}
